{
  "name": "YourHomeBudget",
  "productName": "YourHomeBudget",
  "version": "0.1.0",
  "main": "src/index.js",
  "icon": "res\\ico\\wallet.png",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.6.0",
    "file-system": "^2.2.2",
    "fs-extra": "^4.0.2",
    "stylelint-config-recommended": "^1.0.0"
  },
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=res/ico/wallet.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Your Home Budget\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "electron-titlebar": "^0.0.3"
  }
}
